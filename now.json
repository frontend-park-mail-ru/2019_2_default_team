{
    "version": 1,
    "name": "default_name",
    "alias": ["default-name"],
    "builds": [
      { "src": "server/server.js", "use": "@now/node-server" },
      { "src": "public/**", "use": "@now/static" }
    ],
    "routes": [
      { "src": "/", "dest": "public/index.html" },
      { "src": "/(.*)", "dest": "public/$1" }
    ]
  }