{
    "version": 2,
    "name": "default_name",
    "alias": ["default-name"],
    "builds": [
        { "src": "package.json", "use": "@now/static-build" },
        { "src": "server/server.js", "use": "@now/node-server" },
        { "src": "public/**", "use": "@now/static" }
    ],
    "routes": [
        { "src": "/", "dest": "public/index.html" },
        { "src": "/api/(.*)", "dest": "server/server.js" },
        { "src": "/(.*)", "dest": "public/$1" }
    ]
}